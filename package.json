    "shelljs": "^0.7.0",
    "style-loader": "^0.13.1",
    "ts-helpers": "^1.1.1",
    "tslint": "^3.4.0",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.5.1",
    "typescript": "^2.0.6",
